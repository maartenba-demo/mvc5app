<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>AcmeCompany.Portal</id>
    <title>AcmeCompany.Portal</title>
    <version>$version$</version>
    <authors>Maarten Balliauw</authors>
    <owners>Maarten Balliauw</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Company portal for AcmeCompany. Sample, Hello World MVC5 application</description>
  </metadata>
  <files>
    <file src="bin\*.dll" target="content\bin" />
    <file src="Content\**.*" target="content\Content"/>
    <file src="fonts\**.*" target="content\fonts"/>
    <file src="Scripts\**.*" target="content\Scripts"/>
    <file src="Views\**.*" target="content\Views"/>
    <file src="favicon.ico" target="content"/>
    <file src="Global.asax" target="content"/>
    <file src="Web.config" target="content"/>
    <file src="Web.*.config" target="content"/>
  </files>
</package>